#include "stdafx.h"
#include <iostream>
using namespace std;
int main()
{
	int x[12];
	float x1, x2, x3, d, d1, d2, d3;
	cout << "A1*x1 + B1*x2 + C1*x3 = D1\nA2*x1 + B2*x2 + C2*x3 = D2\nA3*x1 + B3*x2 + C3*x3 = D3" << endl;
	for (int i = 0;i < 12;i++) {
		cin >> x[i];
	}
	d = x[0] * x[5] * x[10] + x[1] * x[6] * x[8]+ x[4] * x[9] * x[2]- x[2] * x[5] * x[8]- x[1] * x[4] * x[10]- x[6] * x[9] * x[0];
	d1 = x[3] * x[5] * x[10] + x[7] * x[9] * x[2] + x[1] * x[6] * x[11] - x[2] * x[5] * x[11] - x[1] * x[7] * x[10] - x[6] * x[9] * x[3];
	d2 = x[0] * x[7] * x[10]+ x[3] * x[6] * x[8]+ x[4] * x[11] * x[2]- x[2] * x[7] * x[8]- x[3] * x[4] * x[10]- x[6] * x[11] * x[0];
	d3 = x[0] * x[5] * x[11]+ x[1] * x[7] * x[9]+ x[4] * x[9] * x[3]- x[3] * x[5] * x[8]- x[1] * x[4] * x[11]- x[7] * x[9] * x[0];
	x1 = d1 / d;
	x2 = d2 / d;
	x3 = d3 / d;
	cout << (float)x1 << " " << (float)x2 << " " << (float)x3 << endl;
	return 0;
}
